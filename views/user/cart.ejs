this is cart page





<table border="5">
    <tr>
        <th>
            ITEM NAME
        </th>
        <th>
            PREVIEW
        </th>
        <th>
            SIZE
        </th>
        <th>
            CATEGORY
        </th>
        <th>
            QUANTUTY
        </th>
        <th>
            SUB TOTAL
        </th>
    </tr>
    <% products.forEach(item => { %>
        <tr>
            <td>
                <%= item.product.name %> 
            </td>
            <td>
                <img src="<%=item.product.imagPaths[0]%> " alt="product image" width="75" height="100">
            </td>
            <td>
                <%= item.size %> 
            </td>
            <td>
                <%= item.product.category %> 
            </td>
            <td>
                <%= item.quantity %> 
            </td>
            <td>
                <%= item.product.price*item.quantity%> 
            </td>
        </tr>
        
    <% }); %> 

</table>